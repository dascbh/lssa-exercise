{
  "name": "apache_latest_role",
  "description": "Install Apache HTTP Server using yum (latest)",
  "json_class": "Chef::Role",
  "default_attributes": {

  },
  "chef_type": "role",
  "run_list": [
    "recipe[apache]"
  ],
  "env_run_lists": {

  },
  "override_attributes": {
    "apache": {
      "httpd": {
        "httpd_user": "daemon",
        "httpd_group": "daemon",
        "install_path": "/etc/httpd",
        "conf_path": "/etc/httpd/conf",
        "httpd_install": "/var/www",
        "httpd_port": "80"
      }
    }
  }
}